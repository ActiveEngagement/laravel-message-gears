{
    "name": "actengage/laravel-message-gears",
    "description": "MessageGears notification drivers for Laravel.",
    "type": "library",
    "authors": [
        {
            "name": "Justin Kimbrell",
            "email": "jkimbrell@actengage.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/config": "^11.0",
        "illuminate/notifications": "^11.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Actengage\\MessageGears\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Actengage\\MessageGears\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
