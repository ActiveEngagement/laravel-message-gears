{
    "name": "actengage/laravel-message-gears",
    "description": "MessageGears notification drivers for Laravel.",
    "type": "library",
    "authors": [
        {
            "name": "Justin Kimbrell",
            "email": "jkimbrell@actengage.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/notifications": "^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "illuminate/support": "^7.0",
        "illuminate/config": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Actengage\\LaravelMessageGears\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Actengage\\LaravelMessageGears\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Actengage\\LaravelMessageGears\\ServiceProvider"
            ]
        }
    }
}
